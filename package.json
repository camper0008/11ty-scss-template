{
    "devDependencies": {
      "@11ty/eleventy": "^1.0.0",
      "sass": "^1.47.0"
    },
    "scripts": {
      "watch:sass": "sass --watch --update --style=expanded src/scss:public/css",
      "build:sass": "sass --no-source-map --style=compressed src/scss:public/css",
      "watch:eleventy": "eleventy --serve",
      "build:eleventy": "eleventy",
      "build": "npm run build:sass && npm run build:eleventy"
    },
    "name": "11ty-scss-template",
    "version": "1.0.0",
    "description": "A template to quickly get started with using 11ty and scss.",
    "repository": "https://github.com/camper0008/11ty-scss-template.git",
    "author": "piet <spetakel123@gmail.com>",
    "license": "MIT"
  }